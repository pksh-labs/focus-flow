{
  "manifest_version": 3,
  "name": "FocusFlow",
  "version": "0.0.1",
  "description": "Productivity-focused website blocker",
  "permissions": [
    "declarativeNetRequest"
  ],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "./popup/index.html",
    "default_icon": "./logo/logo-new.png"
  },
  "background": {
    "service_worker": "./background/background.js"
  },
  "icons": {
    "16": "./logo/logo-new.png",
    "48": "./logo/logo-new.png",
    "128": "./logo/logo-new.png"
  }
}
